{
  "projectType": "library",
  "root": "libs/mfe-poc-lib",
  "sourceRoot": "libs/mfe-poc-lib/src",
  "prefix": "mfe-poc",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/mfe-poc-lib"],
      "options": {
        "project": "libs/mfe-poc-lib/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "./libs/mfe-poc-lib/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/mfe-poc-lib/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/mfe-poc-lib"],
      "options": {
        "jestConfig": "libs/mfe-poc-lib/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/mfe-poc-lib/src/**/*.ts",
          "libs/mfe-poc-lib/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
